@*
    This model import only import a single model
    @model WebApp2ByGiver.Models.Student
*@ 
@*
    This model import only import a enumerable model
*@
@model IEnumerable<WebApp2ByGiver.Models.Student>


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div>
    <h4>Student</h4>
    <hr />
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Address)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Address)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div>
    <a asp-controller="Home" asp-action="Index">Back to List</a>
</div>
